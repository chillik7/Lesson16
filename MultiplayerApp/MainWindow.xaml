<Window x:Class="MultiplayerApp.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Многопользовательское приложение" Height="300" Width="400">


    <Grid>
        <Button Content="Войти" HorizontalAlignment="Center" VerticalAlignment="Center" Width="200" Height="40" Click="OnLoginClick"/>
        <Button Content="Зарегистрироваться" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="200" Height="40" Margin="0,0,0,30" Click="OnRegisterClick"/>
    </Grid>
</Window>
